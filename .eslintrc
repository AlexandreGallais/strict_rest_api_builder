        "@typescript-eslint/no-redeclare": [2],
        "no-regex-spaces": [2],
        "no-return-assign": [2],
        "no-return-await": [0],
        "@typescript-eslint/return-await": [2],
        "no-script-url": [2],
        "no-sequences": [2],
        "no-shadow": [0],
